
{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west9",
      "runWith": {
        "memory": "512MiB",
        "timeoutSeconds": 120,
        "maxInstances": 1
      },
      "secretEnv": [
        "SERVICE_ACCOUNT_PRIVATE_KEY"
      ]
    }
  }
}
